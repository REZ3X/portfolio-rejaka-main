# API Keys
GEMINI_API_KEY=your_gemini_api_key_here
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/database?retryWrites=true&w=majority

# OAuth Configuration - Google
GOOGLE_CLIENT_ID=your_google_client_id_here.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# OAuth Configuration - GitHub  
GITHUB_CLIENT_ID=your_github_client_id_here
GITHUB_CLIENT_SECRET=your_github_client_secret_here

# OAuth Configuration - Discord
DISCORD_CLIENT_ID=your_discord_client_id_here
DISCORD_CLIENT_SECRET=your_discord_client_secret_here

# Uptime Monitoring Configuration
NEXT_PUBLIC_UPTIME_API_URL=your-uptime-api-url

# Server Monitoring Endpoints
NEXT_PUBLIC_REZ3X_SERVER=your-domain
NEXT_PUBLIC_CYX_SERVER=your-domain
NEXT_PUBLIC_XIAN_SERVER=your-domain

# Server Display Names
NEXT_PUBLIC_REZ3X_NAME=REZ3X Server
NEXT_PUBLIC_CYX_NAME=CYX Server
NEXT_PUBLIC_XIAN_NAME=XIAN Server

# Word Filter Configuration (comma-separated)
# English profanity and inappropriate content
NEXT_PUBLIC_BANNED_WORDS_ENGLISH=example_bad_word1,example_bad_word2,example_bad_word3

# Indonesian profanity and inappropriate content  
NEXT_PUBLIC_BANNED_WORDS_INDONESIAN=example_indo_word1,example_indo_word2,example_indo_word3

# Number/symbol variations and leetspeak
NEXT_PUBLIC_BANNED_WORDS_VARIATIONS=3x4mpl3,3x@mpl3,ex4mple

# Spaced and fragmented words
NEXT_PUBLIC_BANNED_WORDS_SPACES=e x a m p l e,e.x.a.m.p.l.e,e-x-a-m-p-l-e

# Common misspellings and typos
NEXT_PUBLIC_BANNED_WORDS_MISSPELLINGS=exampl,exmaple,examlpe

# Sexual content terms
NEXT_PUBLIC_BANNED_WORDS_SEXUAL=example_adult_term1,example_adult_term2

# Violence and harmful content
NEXT_PUBLIC_BANNED_WORDS_VIOLENCE=example_violence_term1,example_violence_term2

# Drug-related terms
NEXT_PUBLIC_BANNED_WORDS_DRUGS=example_drug_term1,example_drug_term2

# Insults and offensive language
NEXT_PUBLIC_BANNED_WORDS_INSULTS=example_insult1,example_insult2

# SSL/TLS Configuration (optional, for production environments)
NODE_TLS_REJECT_UNAUTHORIZED=1
HTTPS_PROXY=
HTTP_PROXY=
NO_PROXY=

# Request Timeout Configuration (in milliseconds)
# Default timeout for OAuth and API requests
REQUEST_TIMEOUT=30000

# Web Environment (Clearnet or Hidden)
NEXT_PUBLIC_WEB_ENVIRONMENT=clearnet

# Domain Setting
NEXT_PUBLIC_CLEARNET_DOMAIN=example.com
NEXT_PUBLIC_HIDDEN_DOMAIN=adsadsad.onion

# Google Analytics ID (for tracking)
NEXT_PUBLIC_GA_ID=your_google_analytics_id_here